<%# <div class = "review_form"> %>
<%= render "layouts/errors", :object => @review %>


<%= form_for [@product, @review] do |f| %>
  <%= f.label :author %>
  <%= f.text_field :author %>
   <%= f.label :content_body %>
  <%= f.text_field :content_body  %>
  <%= f.label "Please pick a rating between 1-5" %>
  <%= f.text_field :rating %>
  <div>
  <br>
  <%= f.collection_check_boxes :product_id, Product.all, :id, :name %> 
  </div>
  <br>
  <p><%= f.submit %></p>
<% end %>
<%# </div> %>